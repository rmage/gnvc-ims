	<parameter name="P_LOCATION" class="java.lang.String"/>
	<parameter name="P_DEALER_CODE" class="java.lang.String">
		<defaultValueExpression><![CDATA["CUSTOM"]]></defaultValueExpression>
	</parameter>
	<parameter name="P_USERNAME" class="java.lang.String"/>
	<parameter name="P_START_DATE" class="java.lang.String"/>
	<parameter name="P_END_DATE" class="java.lang.String"/>
	<parameter name="P_BG_TYPE" class="java.lang.String"/>
	<queryString>
		<![CDATA[select
  AG.TOTAL_AGENT,
  AIS.DOCUMENT_DATE,
  BG.BG_CODE,
  AG.NAME as VENDOR_NAME,
  AIS.DOCUMENT_NO,
  ASD.ITEM_CODE,
  P.NAME as PRODUCT_NAME,
  sum(TS.TOTAL_SOLD) as TOTAL_SOLD,
  sum(AID.TOTAL_ON_HAND) as TOTAL_ON_HAND,
  sum(SNH.TOTAL_ACTIVATED) as TOTAL_ACTIVATED,
  TS.CUSTOMER_PRICE as UNIT_PRICE,
  (sum(TS.TOTAL_SOLD) * TS.CUSTOMER_PRICE) as TOTAL_PRICE
from AGENT_SALES AIS
INNER JOIN AGENT_SALES_DTL ASD on ASD.DOCUMENT_NO=AIS.DOCUMENT_NO
INNER JOIN
(
  select
    ASD.ITEM_CODE,
    ASD.CUSTOMER_PRICE,
    ASD.AGENT_ID,
    sum(ASD.QUANTITY) as TOTAL_SOLD
  from AGENT_SALES_DTL ASD
  group by ASD.ITEM_CODE,ASD.CUSTOMER_PRICE,ASD.AGENT_ID
) TS on TS.ITEM_CODE=ASD.ITEM_CODE and TS.AGENT_ID=ASD.AGENT_ID
INNER join
(
select
  SAD.ITEM_CODE,
  SA.AGENT_ID,
  sum(SAD.BALANCE) as TOTAL_ON_HAND
from STOCK_AGENT SA
INNER JOIN STOCK_AGENT_DTL SAD on SAD.SA_CODE=SA.SA_CODE
group by SAD.ITEM_CODE,SA.AGENT_ID
) AID on AID.ITEM_CODE = ASD.ITEM_CODE and AID.AGENT_ID=ASD.AGENT_ID
LEFT JOIN(
select
  ASN.AGENT_ID,
  SN.BG_CODE,
  SN.ITEM_CODE,
  count(*) as TOTAL_ACTIVATED
from SN_HISTORY SN
INNER JOIN AGENT_SALES_SN ASN on ASN.SN=SN.SN
WHERE SN.EVENT_CODE='ACTIVATED'
group by ASN.AGENT_ID,SN.BG_CODE,SN.ITEM_CODE
) SNH on SNH.AGENT_ID=ASD.AGENT_ID and SNH.ITEM_CODE=ASD.ITEM_CODE
LEFT JOIN AGENT A on A.AGENT_ID=ASD.AGENT_ID
LEFT JOIN
(
select count(*) as TOTAL_AGENT,AG.NAME,AG.SUPPLIER_CODE
from
AGENT_SUPPLIER AG
INNER JOIN AGENT A on A.SUPPLIER_CODE=AG.SUPPLIER_CODE
group by AG.SUPPLIER_CODE,AG.NAME
) AG on AG.SUPPLIER_CODE=A.SUPPLIER_CODE
INNER JOIN BG on BG.BG_CODE=A.BG_CODE
INNER JOIN PRODUCT P on P.ITEM_CODE=ASD.ITEM_CODE
where AIS.DOCUMENT_DATE >= to_date($P{P_START_DATE}||',00:00','dd-mm-yyyy,HH24:MI')
AND AIS.DOCUMENT_DATE <= to_date($P{P_END_DATE}||',23:59','dd-mm-yyyy,HH24:MI')
and BG.BG_CODE like $P{P_DEALER_CODE}
and BG.BRANCH_CODE like $P{P_LOCATION}
and BG.BG_TYPE_CODE like $P{P_BG_TYPE}
group by AG.TOTAL_AGENT,AIS.DOCUMENT_DATE,BG.BG_CODE,AG.NAME,AIS.DOCUMENT_NO,ASD.ITEM_CODE,P.NAME,TS.CUSTOMER_PRICE]]>
	</queryString>
