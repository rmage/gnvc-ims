	<parameter name="P_DOC_NO" class="java.lang.String"/>
	<parameter name="P_DEPOSIT" class="java.lang.String"/>
	<parameter name="P_TERBILANG" class="java.lang.String"/>
	<parameter name="P_PEMBAYARAN_DEPOSIT" class="java.lang.String"/>
	<queryString>
		<![CDATA[select
  PO.INVOICE_NO,
  PD.POSTPAID_DOCUMENT_NO,
  PD.CREATED_DATE as PAYMENT_DATE,
  PD.DOCUMENT_NO,
  sum(SUB1.PAID_CASH) as PAID_CASH ,
  sum(SUB2.PAID_CREDIT) as PAID_CREDIT,
  sum(SUB3.PAID_DEBIT) as PAID_DEBIT,
  sum(SUB4.PAID_OTHER) as PAID_OTHER,
  sum(SUB5.PAID_DEPOSIT) as PAID_DEPOSIT,
  U.NAME as AGENT_NAME,
  BG.NAME as BG_NAME,
  BG.ADDRESS1 as BG_ADDR,
  PO.BILLING_NO,
  PO.BILLING_CUSTOMER,
  PO.CUSTOMER_FNAME,
  PO.CUSTOMER_LNAME,
  PO.MSISDN,
  PO.PAYMENT_TYPE,
  B.NAME as LOCATION
from POSTPAID_DTL PD
left join (
select
  PD.DOCUMENT_NO,
  PD.TRANS_DATE,
  PD.METHOD_CODE,
  PD.AMOUNT as PAID_CASH
from POSTPAID_DTL PD
where PD.METHOD_CODE='CASH'
) SUB1 on SUB1.DOCUMENT_NO=PD.DOCUMENT_NO
left join (
select
  PD.DOCUMENT_NO,
  PD.TRANS_DATE,
  PD.METHOD_CODE,
  PD.AMOUNT as PAID_CREDIT
from POSTPAID_DTL PD
where PD.METHOD_CODE='CREDIT_CARD'
) SUB2 on SUB2.DOCUMENT_NO=PD.DOCUMENT_NO
left join (
select
  PD.DOCUMENT_NO,
  PD.TRANS_DATE,
  PD.METHOD_CODE,
  PD.AMOUNT as PAID_DEBIT
from POSTPAID_DTL PD
where PD.METHOD_CODE='DEBIT_CARD'
) SUB3 on SUB3.DOCUMENT_NO=PD.DOCUMENT_NO
left join (
select
  PD.DOCUMENT_NO,
  PD.TRANS_DATE,
  PD.METHOD_CODE,
  PD.AMOUNT as PAID_OTHER
from POSTPAID_DTL PD
where PD.METHOD_CODE='OTHER'
) SUB4 on SUB4.DOCUMENT_NO=PD.DOCUMENT_NO
left join (
select
  PD.DOCUMENT_NO,
  PD.TRANS_DATE,
  PD.METHOD_CODE,
  PD.AMOUNT as PAID_DEPOSIT
from POSTPAID_DTL PD
where PD.METHOD_CODE='DEPOSIT'
) SUB5 on SUB5.DOCUMENT_NO=PD.DOCUMENT_NO
INNER JOIN POSTPAID PO on PO.DOCUMENT_NO=PD.POSTPAID_DOCUMENT_NO
INNER JOIN "USER" U on U.USER_ID=PD.CREATED_BY
INNER JOIN BG on BG.BG_CODE=PO.BG_CODE
left join BRANCH B on B.BRANCH_CODE=BG.BRANCH_CODE
--LEFT JOIN CUSTOMER C on C.CUSTOMER_ID=S.CUSTOMER_ID
where PD.METHOD_CODE in ('CASH','CREDIT_CARD','DEBIT_CARD','OTHER','DEPOSIT')
and PD.DOCUMENT_NO=$P{P_DOC_NO}
group by PO.INVOICE_NO,PD.POSTPAID_DOCUMENT_NO,PD.CREATED_DATE,PD.DOCUMENT_NO,BG.NAME,BG.ADDRESS1,U.NAME,
PO.BILLING_NO,PO.BILLING_CUSTOMER,PO.MSISDN,PO.PAYMENT_TYPE,B.NAME,PO.CUSTOMER_FNAME,PO.CUSTOMER_LNAME]]>
	</queryString>
