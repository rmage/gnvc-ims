	<queryString>
		<![CDATA[select s.document_date as docdate,
 to_date(s.document_date,'dd-mm-YY') as doc_date,
s.user_id,
u.user_id,
u.name as agent_name,
s.bg_code,
s.document_no as invoice,
sp.document_no as receipt,
sdp.item_code,
p.item_code as item_name,
p.name as item_alias,
p.catalog_code as item_type,
sdsn.sn as SN,
sdidn.ident_no as ident,
CASE WHEN P.CATALOG_CODE IN ('S') THEN  sdsn.sn  ELSE CASE WHEN
P.CATALOG_CODE IN ('CS') THEN sdidn.ident_no END END AS START_SERIAL,
sdp.catalog_code as price_category,
sdp.value_price,
sdp.value_vat,
sdp.customer_price
from sales s
--sales_payment SP,
--sales_dtl_product sdp,
--sales_dtl_sn sdsn,
--product p, "USER" u

left join sales_payment SP on sp.sales_document_no = s.document_no

left join sales_dtl_product sdp on sdp.document_no = s.document_no
left join sales_dtl_sn sdsn on sdsn.document_no = sdp.document_no and sdsn.item_code = sdp.item_code and sdsn.catalog_code = sdp.catalog_code
left join sales_dtl_ident sdidn on sdidn.document_no = sdp.document_no and sdidn.item_code = sdp.item_code and sdidn.catalog_code = sdp.catalog_code
left join product p on  P.ITEM_CODE = sdp.ITEM_CODE
left join "USER" u on s.user_id = u.USER_ID
where s.status_code = 'PAID'
and s.bg_code like $P{P_DEALER_CODE}
and P.CATALOG_CODE like $P{P_ITEM_TYPE}
and to_date(s.document_date,'dd-mm-YY') >= to_date($P{P_START_DATE},'dd-mm-YY')
and to_date(s.document_date,'dd-mm-YY') <= to_date($P{P_END_DATE},'dd-mm-YY')
order by p.catalog_code,s.document_date, s.bg_code, u.name, p.name]]>
	</queryString>
