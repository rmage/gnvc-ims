	<queryString>
		<![CDATA[select
  trunc(EVENT_DATE,'MON') as EVENT_DATE,
  case when to_char(EVENT_DATE,'MM')='01' then 'January'
  when to_char(EVENT_DATE,'MM')='02' then 'February'
  when to_char(EVENT_DATE,'MM')='03' then 'March'
  when to_char(EVENT_DATE,'MM')='04' then 'April'
  when to_char(EVENT_DATE,'MM')='05' then 'May'
  when to_char(EVENT_DATE,'MM')='06' then 'June'
  when to_char(EVENT_DATE,'MM')='07' then 'July'
  when to_char(EVENT_DATE,'MM')='08' then 'August'
  when to_char(EVENT_DATE,'MM')='09' then 'September'
  when to_char(EVENT_DATE,'MM')='10' then 'October'
  when to_char(EVENT_DATE,'MM')='11' then 'November'
  when to_char(EVENT_DATE,'MM')='12' then 'December'
  end as MON,
  EVENT_CODE,
  count(*) as TOTAL_EVENT
from SN_HISTORY SNH
INNER JOIN BG on BG.BG_CODE=SNH.BG_CODE
where to_char(EVENT_DATE,'yyyy')=$P{P_YEAR}
and SNH.BG_CODE like $P{P_DEALER_CODE}
AND BG.BRANCH_CODE like $P{P_LOCATION}
AND BG.BG_TYPE_CODE like $P{P_BG_TYPE}
group by EVENT_CODE,trunc(EVENT_DATE,'MON'),to_char(EVENT_DATE,'MM')
order by trunc(EVENT_DATE,'MON')]]>
	</queryString>
