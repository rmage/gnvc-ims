		<defaultValueExpression><![CDATA["CUSTOM"]]></defaultValueExpression>
	</parameter>
	<parameter name="P_USERNAME" class="java.lang.String"/>
	<parameter name="P_BG_TYPE" class="java.lang.String"/>
	<queryString>
		<![CDATA[select * from
(
select
  C.CASH_CODE,
  C.BG_CODE,
  BG.NAME as BG_NAME,
  BG.BRANCH_CODE,
  BG.BG_TYPE_CODE,
  CD.CASH_TYPE_CODE,
  CD.START_BALANCE as OPENING_BALANCE,
  CD.BALANCE as END_BALANCE,
  CD.METHOD_CODE,
  coalesce(CIN.AMOUNT,0) as CASH_IN,
  coalesce(COUT.AMOUNT,0) as CASH_OUT
from CASH C
INNER join CASH_DTL CD on C.CASH_CODE=CD.CASH_CODE
LEFT join
(
  select
    CT.CASH_CODE,
    CT.CASH_TYPE_CODE,ct.METHOD_CODE,
    sum(AMOUNT) as AMOUNT
  from CASH_TRANS CT
  WHERE CT.CASH_TRANS_CODE like '%IN%'
  group by CT.CASH_CODE, CT.CASH_TYPE_CODE, ct.METHOD_CODE
) CIN on CIN.CASH_CODE=C.CASH_CODE and CIN.CASH_TYPE_CODE=CD.CASH_TYPE_CODE AND CIN.METHOD_CODE=CD.METHOD_CODE
LEFT join(
  select
    CT.CASH_CODE,
    CT.CASH_TYPE_CODE,ct.METHOD_CODE,
    sum(AMOUNT) as AMOUNT
  from CASH_TRANS CT
  WHERE CT.CASH_TRANS_CODE like '%OUT%'
  group by CT.CASH_CODE,CT.CASH_TYPE_CODE,ct.METHOD_CODE
) COUT on COUT.CASH_CODE=C.CASH_CODE and COUT.CASH_TYPE_CODE=CD.CASH_TYPE_CODE AND COUT.METHOD_CODE=CD.METHOD_CODE
INNER JOIN CASH_TYPE CTYPE on CD.CASH_TYPE_CODE=CTYPE.CASH_TYPE_CODE
inner join BG on BG.BG_CODE=C.BG_CODE
inner join BRANCH B on B.BRANCH_CODE=BG.BRANCH_CODE
where C.IS_CURRENT='Y'
union
select
  PET.PETTY_CODE,
  PET.BG_CODE,
  BG.NAME as BG_NAME,
  BG.BRANCH_CODE,
  BG.BG_TYPE_CODE,
  'PETTY CASH' as CASH_TYPE_CODE,
  PET.START_BALANCE as OPENING_BALANCE,
  PET.BALANCE as END_BALANCE,
  'CASH' AS METHOD_CODE,
  COALESCE(PIN.AMOUNT,0) as CASH_IN,
  COALESCE(POUT.AMOUNT,0) as CASH_OUT
from PETTY PET
left join(
  select
    PIN.PETTY_CODE,
    --CT.CASH_TYPE_CODE,
    --CT.CASH_TRANS_CODE,
    sum(AMOUNT) as AMOUNT
  from PETTY_IN PIN
  group by PIN.PETTY_CODE
) PIN on PET.PETTY_CODE=PIN.PETTY_CODE
left join(
  select
    PETTY_CODE,
    --CT.CASH_TYPE_CODE,
    --CT.CASH_TRANS_CODE,
    sum(AMOUNT) as AMOUNT
  from PETTY_OUT
  group by PETTY_CODE
) POUT on PET.PETTY_CODE=POUT.PETTY_CODE
inner join BG on BG.BG_CODE=PET.BG_CODE
inner join BRANCH B on B.BRANCH_CODE=BG.BRANCH_CODE
where IS_CURRENT='Y'
)
where (CASH_IN > 0 or CASH_OUT >0)
AND BG_CODE LIKE $P{P_DEALER_CODE}
AND BRANCH_CODE like $P{P_LOCATION}
AND BG_TYPE_CODE like $P{P_BG_TYPE}]]>
