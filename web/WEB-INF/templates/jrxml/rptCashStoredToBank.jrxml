	<queryString>
		<![CDATA[select
C.BG_CODE AS BG_CODE
,BG.NAME as BG_NAME
,trunc(CT.TRANS_DATE) AS TRANS_DATE
,CT.DESCRIPTION AS DESCRIPTION
,CT.AMOUNT AS AMOUNT
,CT.METHOD_CODE
from CASH_TRANS CT
INNER join CASH_TRANS_TYPE CTP on CT.CASH_TRANS_CODE=CTP.CASH_TRANS_CODE
INNER join CASH C on C.CASH_CODE=CT.CASH_CODE
INNER join BG on BG.BG_CODE=C.BG_CODE
INNER join PAYMENT_METHOD PM on PM.METHOD_CODE=CT.METHOD_CODE
where CTP.CASH_TRANS_CODE='OUT_BANK'
and (CT.TRANS_DATE >= to_date($P{P_START_DATE}||',00:00:00','dd-mm-yyyy,HH24:MI:ss')
and CT.TRANS_DATE <= to_date( $P{P_END_DATE}||',23:59:59','dd-mm-yyyy,HH24:MI:ss'))
and C.BG_CODE like $P{P_DEALER_CODE}
and BG.BRANCH_CODE like $P{P_LOCATION}
and BG.BG_TYPE_CODE like $P{P_BG_TYPE}
ORDER BY C.BG_CODE, CT.TRANS_DATE]]>
	</queryString>
