	<queryString>
		<![CDATA[select
  R.RESERVATION_NO,
  R.RESERVATION_DATE,
  R.SALES_DOCUMENT_NO,
  C.NAME as CUST_NAME,
  C.ADDRESS as CUST_ADDR,
  C.PHONE as CUST_PHONE,
  BG.NAME as STORE_NAME,
  BG.ADDRESS1 as STORE_ADDR,
  A.NAME as AGENT_NAME,
  S.DOCUMENT_DATE as DELIVERY_DATE,
  (nvl(P.NAME,SVC.NAME)) as ITEM_NAME,
  P.ITEM_CODE,
  SUBP.QUANTITY,
  SUBP.CUSTOMER_PRICE,
  SUBP.VALUE_PRICE,
  SUBP.VALUE_VAT,
  SUBP.SN_START,
  SUBP.SN_END
from RESERVATION R
left JOIN CUSTOMER C on C.CUSTOMER_ID=R.CUSTOMER_ID
left join BG on BG.BG_CODE=R.BG_CODE
left join SALES S on S.DOCUMENT_NO=R.SALES_DOCUMENT_NO
left join AGENT A on A.AGENT_ID=S.AGENT_ID
left join(
  select
      SDBD.DOCUMENT_NO,
      SDBD.ITEM_CODE,
      SDBD.QUANTITY,
      (SDB.VALUE_VAT + SDB.VALUE_PRICE)CUSTOMER_PRICE,
      SDBD.VALUE_VAT,
      SDBR.SN_START,
      SDBR.SN_END,
      SDB.VALUE_PRICE
  from SALES_DTL_BUNDLE_DTL SDBD
  left join SALES_DTL_BUNDLE_RANGE SDBR on SDBR.DOCUMENT_NO=SDBD.DOCUMENT_NO
  left join SALES_DTL_BUNDLE SDB on SDB.DOCUMENT_NO=SDBD.DOCUMENT_NO
  and SDBR.CATALOG_CODE=SDBD.CATALOG_CODE
  and SDBR.ITEM_CODE=SDBD.ITEM_CODE
  union
  select
    SDP.DOCUMENT_NO,
    SDP.ITEM_CODE,
    SDP.QUANTITY,
    SDP.CUSTOMER_PRICE,
    SDP.VALUE_VAT,
    SDR.SN_START,
    SDR.SN_END,
    SDP.VALUE_PRICE
  from SALES_DTL_PRODUCT SDP
  left join SALES_DTL_RANGE SDR on SDR.DOCUMENT_NO=SDP.DOCUMENT_NO
  and SDR.CATALOG_CODE=SDP.CATALOG_CODE
  and SDR.ITEM_CODE=SDP.ITEM_CODE
  union
  select
    SDS.DOCUMENT_NO,
    SDS.SERVICE_CODE as ITEM_CODE,
    SDS.QUANTITY,
    SDS.CUSTOMER_PRICE,
    SDS.VALUE_VAT,
    '--' as SN_START,
    '--' as SN_END,
    SDS.VALUE_PRICE
  from SALES_DTL_SERVICE SDS
) SUBP on SUBP.DOCUMENT_NO=R.SALES_DOCUMENT_NO
left join PRODUCT P on P.ITEM_CODE=SUBP.ITEM_CODE
left join SERVICE SVC on SVC.SERVICE_CODE=SUBP.ITEM_CODE
where R.RESERVATION_NO=$P{P_DOC_NO}]]>
	</queryString>
