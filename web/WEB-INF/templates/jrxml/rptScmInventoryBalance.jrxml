	<queryString>
		<![CDATA[select
BG.NAME as STORE_NAME,
WH.WH_CODE,
WH.NAME as WH_NAME,
SWH.ITEM_CODE,
P.NAME as ITEM_NAME,
SWH.BALANCE,
S.START_DATE as CUR_DATE
from STOCK_WH SWH
INNER JOIN STOCK S ON S.STOCK_CODE=SWH.STOCK_CODE
INNER JOIN PRODUCT P ON SWH.ITEM_CODE=P.ITEM_CODE
INNER JOIN WH ON WH.WH_CODE=SWH.WH_CODE
INNER JOIN BG on BG.BG_CODE=S.BG_CODE
where S.IS_CURRENT='Y'
and BG.BG_CODE like $P{P_DEALER_CODE}
and BG.BRANCH_CODE like $P{P_LOCATION}
and BG.BG_TYPE_CODE like $P{P_BG_TYPE}
order by BG.BG_CODE, WH.WH_CODE, ITEM_CODE]]>
