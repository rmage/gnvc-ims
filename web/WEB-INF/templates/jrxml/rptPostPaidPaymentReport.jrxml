	<queryString>
		<![CDATA[SELECT D.CREATED_DATE AS RECEIVED_DATE
,P.BG_CODE AS BG_CODE
,BG.NAME as BG_NAME
,D.DOCUMENT_NO AS RECEIPT_NO
,P.MSISDN AS MSISDN
,P.BILLING_NO AS BILLING_NO
,COALESCE((COALESCE(P.CUSTOMER_FNAME || ' ','') || P.CUSTOMER_LNAME),'-') AS BILLING_NAME
,D.METHOD_CODE
,D.AMOUNT AS PAYMENT_AMOUNT
,CASE WHEN P.PAYMENT_TYPE IN ('FULL_PAID','LESS_PAID') THEN 'PAYMENT' ELSE P.PAYMENT_TYPE END AS TRANS_TYPE_CODE
FROM POSTPAID P
INNER JOIN BG BG ON P.BG_CODE=BG.BG_CODE
INNER JOIN POSTPAID_DTL D ON P.DOCUMENT_NO=D.POSTPAID_DOCUMENT_NO
INNER JOIN BRANCH B ON BG.BRANCH_CODE=B.BRANCH_CODE
where
P.PAYMENT_TYPE NOT LIKE '%CANCEL%'
AND D.TRANS_DATE >= to_date($P{P_START_DATE}||',00:00:00','dd-mm-yyyy,HH24:MI:ss')
and D.TRANS_DATE <= to_date($P{P_END_DATE}||',23:59:59','dd-mm-yyyy,HH24:MI:ss')
and BG.BG_CODE like $P{P_DEALER_CODE}
and BG.BRANCH_CODE like $P{P_LOCATION}
and BG.BG_TYPE_CODE like $P{P_BG_TYPE}
ORDER BY BG.BG_CODE,TRANS_TYPE_CODE ,D.METHOD_CODE, D.TRANS_DATE]]>
	</queryString>
