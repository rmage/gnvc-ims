	<parameter name="P_DOC_NO" class="java.lang.String"/>
	<parameter name="P_DEPOSIT" class="java.lang.String"/>
	<parameter name="P_TERBILANG" class="java.lang.String"/>
	<parameter name="P_PEMBAYARAN_DEPOSIT" class="java.lang.String"/>
	<queryString>
		<![CDATA[select
  SP.SALES_DOCUMENT_NO,
  trunc(SP.DOCUMENT_DATE) as PAYMENT_DATE,
  SPD.DOCUMENT_NO,
  sum(SUB1.PAID_CASH) as PAID_CASH ,
  sum(SUB2.PAID_CREDIT) as PAID_CREDIT,
  sum(SUB3.PAID_DEBIT) as PAID_DEBIT,
  sum(SUB4.PAID_OTHER) as PAID_OTHER,
  sum(SUB5.PAID_DEPOSIT) as PAID_DEPOSIT,
  U.NAME as AGENT_NAME,
  BG.NAME as BG_NAME,
  BG.ADDRESS1 as BG_ADDR,
  C.NAME as CUST_NAME,
  C.NPWP as CUST_NPWP,
  C.ADDRESS as CUST_ADDR,
  C.PHONE as CUST_PHONE,
  B.NAME as LOCATION
from SALES_PAYMENT SPD
left join (
select
  SPD.DOCUMENT_NO,
  SPD.TRANS_DATE,
  SPD.METHOD_CODE,
  SPD.AMOUNT as PAID_CASH
from SALES_PAYMENT_DTL SPD
where SPD.METHOD_CODE='CASH'
) SUB1 on SUB1.DOCUMENT_NO=SPD.DOCUMENT_NO
left join (
select
  SPD.DOCUMENT_NO,
  SPD.TRANS_DATE,
  SPD.METHOD_CODE,
  SPD.AMOUNT as PAID_CREDIT
from SALES_PAYMENT_DTL SPD
where SPD.METHOD_CODE='CREDIT_CARD'
) SUB2 on SUB2.DOCUMENT_NO=SPD.DOCUMENT_NO
left join (
select
  SPD.DOCUMENT_NO,
  SPD.TRANS_DATE,
  SPD.METHOD_CODE,
  SPD.AMOUNT as PAID_DEBIT
from SALES_PAYMENT_DTL SPD
where SPD.METHOD_CODE='DEBIT_CARD'
) SUB3 on SUB3.DOCUMENT_NO=SPD.DOCUMENT_NO
left join (
select
  SPD.DOCUMENT_NO,
  SPD.TRANS_DATE,
  SPD.METHOD_CODE,
  SPD.AMOUNT as PAID_OTHER
from SALES_PAYMENT_DTL SPD
where SPD.METHOD_CODE='OTHER'
) SUB4 on SUB4.DOCUMENT_NO=SPD.DOCUMENT_NO
left join (
select
  SPD.DOCUMENT_NO,
  SPD.TRANS_DATE,
  SPD.METHOD_CODE,
  SPD.AMOUNT as PAID_DEPOSIT
from SALES_PAYMENT_DTL SPD
where SPD.METHOD_CODE='DEPOSIT'
) SUB5 on SUB5.DOCUMENT_NO=SPD.DOCUMENT_NO
LEFT JOIN SALES_PAYMENT SP on SP.DOCUMENT_NO=SPD.DOCUMENT_NO
LEFT JOIN "USER" U on U.USER_ID=SPD.CREATED_BY
LEFT JOIN SALES S on S.DOCUMENT_NO=SP.SALES_DOCUMENT_NO
LEFT JOIN BG on BG.BG_CODE=S.BG_CODE
left join BRANCH B on B.BRANCH_CODE=BG.BRANCH_CODE
LEFT JOIN CUSTOMER C on C.CUSTOMER_ID=S.CUSTOMER_ID
where SP.DOCUMENT_NO=$P{P_DOC_NO}
group by SP.SALES_DOCUMENT_NO,trunc(SP.DOCUMENT_DATE),SPD.DOCUMENT_NO,BG.NAME,BG.ADDRESS1,U.NAME,C.NAME,C.NPWP,C.ADDRESS,C.PHONE,B.NAME]]>
