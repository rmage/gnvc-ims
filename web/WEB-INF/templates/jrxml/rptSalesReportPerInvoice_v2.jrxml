	<parameter name="P_BG_TYPE" class="java.lang.String"/>
	<parameter name="P_DEALER_CODE" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
BG.BG_CODE AS BG_CODE
,BG.NAME as BG_NAME
,B."NAME" AS AREA_NAME
,S.DOCUMENT_DATE AS SALES_DATE
,S.DOCUMENT_NO AS INVOICE_NO
,P.ITEM_CODE AS ITEM_CODE
,PC."NAME" AS PRODUCT_CATEGORY
,P."NAME" AS ITEM_NAME
,CU."NAME" AS CUSTOMER_NAME
,D.QUANTITY AS QUANTITY
,D.CUSTOMER_PRICE AS ITEM_PRICE --HARGA SATUAN
,D.TOTAL_PRICE AS TOTAL_PRICE
,D.DISCOUNT_PERCENT AS DISCOUNT_PERCENT
,D.DISCOUNT_PRICE AS DISCOUNT_AMOUNT
,D.VALUE_VAT AS VALUE_VAT -- INI UDAH GABUNGAN BELUM YA ?
,D.VALUE_PRICE AS DISCOUNTED_PRICE
,D.SN_START AS SERIAL_START -- YAKIN CARANYA
,D.SN_END AS SERIAL_END -- BELUM YAKIN CARA NYA
,(D.QUANTITY * D.CUSTOMER_PRICE) AS CUSTOMER_PRICE
,U.NAME as AGENT_NAME
FROM SALES S
INNER JOIN BG BG ON S.BG_CODE=BG.BG_CODE
LEFT JOIN BRANCH B ON BG.BRANCH_CODE=B.BRANCH_CODE
LEFT JOIN CUSTOMER CU ON S.CUSTOMER_ID=CU.CUSTOMER_ID
LEFT JOIN "USER" U ON U.USER_ID=S.USER_ID
LEFT JOIN
  (SELECT SD.DOCUMENT_NO
  ,SD.ITEM_CODE
  ,SD.QUANTITY
  ,SD.BASE_PRICE
  ,SD.QUANTITY * SD.CUSTOMER_PRICE AS TOTAL_PRICE
  ,SD.DISCOUNT_PERCENT
  ,SD.DISCOUNT_PRICE
  ,SD.VALUE_VAT
  ,SD.VALUE_PRICE
  ,SD.CUSTOMER_PRICE AS CUSTOMER_PRICE
  ,SDR.SN_START
  ,SDR.SN_END
  FROM SALES_DTL_PRODUCT SD
  LEFT JOIN SALES_DTL_RANGE SDR on SD.CATALOG_CODE=SDR.CATALOG_CODE AND
  SD.ITEM_CODE=SDR.ITEM_CODE AND
  SD.DOCUMENT_NO=SDR.DOCUMENT_NO
  UNION
  SELECT SD.DOCUMENT_NO
  ,SDP.ITEM_CODE
  ,SDP.QUANTITY
  ,SDP.BASE_PRICE
  ,SDP.QUANTITY * (SDP.VALUE_VAT + SDP.VALUE_PRICE) AS TOTAL_PRICE
  ,SDP.DISCOUNT_PERCENT
  ,SDP.DISCOUNT_PRICE
  ,SDP.VALUE_VAT
  ,SDP.VALUE_PRICE
  ,SD.CUSTOMER_PRICE
  ,SDR.SN_START
  ,SDR.SN_END
  FROM SALES_DTL_BUNDLE SD
  LEFT JOIN SALES_DTL_BUNDLE_DTL SDP ON SD.DOCUMENT_NO=SDP.DOCUMENT_NO
  AND SD.BUNDLE_CODE=SDP.BUNDLE_CODE
  AND SD.CATALOG_CODE=SDP.CATALOG_CODE
  LEFT JOIN SALES_DTL_RANGE SDR ON SDP.DOCUMENT_NO=SDR.DOCUMENT_NO
  AND SDP.ITEM_CODE=SDR.ITEM_CODE
  AND SDP.CATALOG_CODE=SDR.CATALOG_CODE
  ) D ON S.DOCUMENT_NO=D.DOCUMENT_NO
INNER JOIN PRODUCT P ON D.ITEM_CODE=P.ITEM_CODE
LEFT JOIN CUSTOMER_BG CBG on BG.BG_CODE=CBG.BG_CODE
LEFT JOIN PRODUCT_CATEGORY PC on PC.CATEGORY_CODE=P.CATEGORY_CODE
where S.DOCUMENT_DATE >= to_date($P{P_START_DATE}||',00:00','dd-mm-yyyy,HH24:MI')
and S.DOCUMENT_DATE <= to_date($P{P_END_DATE}||',23:59','dd-mm-yyyy,HH24:MI')
and B.BRANCH_CODE like $P{P_LOCATION}
and BG.BG_TYPE_CODE like $P{P_BG_TYPE}
and BG.BG_CODE like $P{P_DEALER_CODE}
and S.STATUS_CODE = 'PAID'
union
SELECT
BG.BG_CODE AS BG_CODE
,BG.NAME as BG_NAME
,B."NAME" AS AREA_NAME
,S.DOCUMENT_DATE AS SALES_DATE
,S.DOCUMENT_NO AS INVOICE_NO
,P.ITEM_CODE AS ITEM_CODE
,PC."NAME" AS PRODUCT_CATEGORY
,P."NAME" AS ITEM_NAME
,'' AS CUSTOMER_NAME
,D.QUANTITY AS QUANTITY
,D.CUSTOMER_PRICE AS ITEM_PRICE --HARGA SATUAN
,D.TOTAL_PRICE AS TOTAL_PRICE
,D.DISCOUNT_PERCENT AS DISCOUNT_PERCENT
,D.DISCOUNT_PRICE AS DISCOUNT_AMOUNT
,D.VALUE_VAT AS VALUE_VAT -- INI UDAH GABUNGAN BELUM YA ?
,D.VALUE_PRICE AS DISCOUNTED_PRICE
,D.SN_START AS SERIAL_START -- YAKIN CARANYA
,D.SN_END AS SERIAL_END -- BELUM YAKIN CARA NYA
,(D.QUANTITY * D.CUSTOMER_PRICE) AS CUSTOMER_PRICE
,'' AS AGENT_NAME
FROM AGENT_SALES S
INNER JOIN BG BG ON S.BG_CODE=BG.BG_CODE
LEFT JOIN BRANCH B ON BG.BRANCH_CODE=B.BRANCH_CODE
--LEFT JOIN CUSTOMER CU ON S.CUSTOMER_ID=CU.CUSTOMER_ID
LEFT JOIN
  (
  select
  ASD.DOCUMENT_NO
  ,ASD.ITEM_CODE
  ,ASD.QUANTITY
  ,ASD.BASE_PRICE
  ,ASD.QUANTITY * ASD.CUSTOMER_PRICE AS TOTAL_PRICE
  ,ASD.DISCOUNT_PERCENT
  ,ASD.DISCOUNT_PRICE
  ,ASD.VALUE_VAT
  ,ASD.VALUE_PRICE
  ,ASD.CUSTOMER_PRICE
  ,ASR.SN_START
  ,ASR.SN_END
  ,ASD.AGENT_ID
  from AGENT_SALES_DTL ASD
  left join AGENT_SALES_RANGE ASR on ASR.DOCUMENT_NO=ASD.DOCUMENT_NO and ASR.ITEM_CODE=ASD.ITEM_CODE
  ) D ON S.DOCUMENT_NO=D.DOCUMENT_NO
INNER JOIN PRODUCT P ON D.ITEM_CODE=P.ITEM_CODE
LEFT JOIN CUSTOMER_BG CBG on BG.BG_CODE=CBG.BG_CODE
LEFT JOIN PRODUCT_CATEGORY PC on PC.CATEGORY_CODE=P.CATEGORY_CODE
where S.DOCUMENT_DATE >= to_date($P{P_START_DATE}||',00:00','dd-mm-yyyy,HH24:MI')
and S.DOCUMENT_DATE <= to_date($P{P_END_DATE}||',23:59','dd-mm-yyyy,HH24:MI')
and B.BRANCH_CODE like $P{P_LOCATION}
and BG.BG_TYPE_CODE like $P{P_BG_TYPE}
and BG.BG_CODE like $P{P_DEALER_CODE}
order by BG_CODE,SALES_DATE]]>
