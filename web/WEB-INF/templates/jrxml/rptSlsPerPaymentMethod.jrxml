	<parameter name="P_USERNAME" class="java.lang.String"/>
	<queryString>
		<![CDATA[select to_date( s.document_date,'dd-mm-YY') as doc_date,
s.user_id,
u.user_id,
u.name as agent_name,
s.bg_code,
s.document_no as invoice,
sp.document_no as receipt,
sdp.quantity,
sdp.catalog_code as price_category,
sdp.value_price as price_amount,
sdp.value_vat as price_vat,
sdp.customer_price,
spd.method_code,
CASE WHEN spd.method_code IN ('CASH') THEN spd.AMOUNT ELSE 0 END AS CASH
,CASE WHEN spd.method_code IN ('CREDIT_CARD') THEN spd.AMOUNT ELSE 0 END AS CREDIT_CARD
,CASE WHEN spd.method_code IN ('DEBIT_CARD') THEN spd.AMOUNT ELSE 0 END AS DEBIT_CARD
from sales s
left join sales_payment SP on sp.sales_document_no = s.document_no
left join sales_dtl_product sdp on sdp.document_no = s.document_no
left join sales_payment_dtl spd on SP.document_no = spd.document_no and spd.amount = SP.amount
left join "USER" u on s.user_id = u.USER_ID
where s.status_code = 'PAID'
and s.bg_code like $P{P_DEALER_CODE}
and to_date(s.document_date,'dd-mm-YY') >= to_date($P{P_START_DATE},'dd-mm-YY')
and to_date(s.document_date,'dd-mm-YY') <= to_date($P{P_END_DATE},'dd-mm-YY')
order by s.document_date, s.bg_code]]>
	</queryString>
