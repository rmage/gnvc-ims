	<queryString>
		<![CDATA[select
C.BG_CODE AS BG_CODE
,BG.NAME AS BG_NAME
,trunc(CT.TRANS_DATE) AS TRANS_DATE
,PM.NAME AS PAYMENT_METHOD
,CT.DESCRIPTION AS DESCRIPTION
,CT.AMOUNT AS AMOUNT
from CASH_TRANS CT
INNER JOIN CASH_TRANS_TYPE CTT on CTT.CASH_TRANS_CODE=CT.CASH_TRANS_CODE
INNER JOIN CASH C on C.CASH_CODE=CT.CASH_CODE
INNER JOIN PAYMENT_METHOD PM on CT.METHOD_CODE=PM.METHOD_CODE
INNER JOIN BG on C.BG_CODE=BG.BG_CODE
where CTT.CASH_TRANS_CODE IN('IN_POSTPAID_PAYMENT','IN_SALES_PAYMENT')
and CT.METHOD_CODE NOT IN('CASH')
and CT.TRANS_DATE >= to_date($P{P_START_DATE}||',00:00:00','dd-mm-yyyy,HH24:MI:SS')
and CT.TRANS_DATE <= to_date($P{P_END_DATE}||',23:59:59','dd-mm-yyyy,HH24:MI:SS')
and BG.BG_CODE like $P{P_DEALER_CODE}
and BG.BRANCH_CODE like $P{P_LOCATION}
and BG.BG_TYPE_CODE like $P{P_BG_TYPE}
order by BG_CODE asc, PM.NAME asc, CT.TRANS_DATE asc]]>
