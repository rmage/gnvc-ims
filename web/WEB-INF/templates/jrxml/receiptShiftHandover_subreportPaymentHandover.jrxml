	<parameter name="P_DOC_NO" class="java.lang.String"/>
	<queryString>
		<![CDATA[select
  CM.DOCUMENT_NO,
  CMD.METHOD_CODE,
  CMD.AMOUNT as CASH,
  CC.AMOUNT as CREDIT,
  CC.AMOUNT as DEBIT,
  D.AMOUNT as DEPOSIT,
  CO.AMOUNT as OTHERS,
  (CMD.AMOUNT+CC.AMOUNT+CD.AMOUNT+D.AMOUNT+CO.AMOUNT) as TOTAL_AMOUNT
from CASH_MOVE CM
left join CASH_MOVE_DTL CMD on CM.DOCUMENT_NO=CMD.DOCUMENT_NO
left join (
  select
    CM.DOCUMENT_NO,
    CM.METHOD_CODE,
    CM.AMOUNT
  from CASH_MOVE_DTL CM where CM.METHOD_CODE='CREDIT'
  ) CC on CC.DOCUMENT_NO=CM.DOCUMENT_NO
left join (
  select
    CM.DOCUMENT_NO,
    CM.METHOD_CODE,
    CM.AMOUNT
  from CASH_MOVE_DTL CM where CM.METHOD_CODE='DEBIT'
  ) CD on CD.DOCUMENT_NO=CM.DOCUMENT_NO
left join (
  select
    CM.DOCUMENT_NO,
    CM.METHOD_CODE,
    CM.AMOUNT
  from CASH_MOVE_DTL CM where CM.METHOD_CODE='DEPOSIT'
  ) D on D.DOCUMENT_NO=CM.DOCUMENT_NO
left join (
  select
    CM.DOCUMENT_NO,
    CM.METHOD_CODE,
    CM.AMOUNT
  from CASH_MOVE_DTL CM where CM.METHOD_CODE='DEPOSIT'
  ) CO on CO.DOCUMENT_NO=CM.DOCUMENT_NO
where METHOD_CODE='CASH'
and CM.DOCUMENT_NO=$P{P_DOC_NO}]]>
	</queryString>
