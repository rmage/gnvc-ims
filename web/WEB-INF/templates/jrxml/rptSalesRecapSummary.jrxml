	<queryString>
		<![CDATA[(select
S.DOCUMENT_DATE as SALES_DATE
,S.DOCUMENT_NO as INVOICE_NO
,SDP.ITEM_CODE
,sum(SDP.QUANTITY) as QUANTITY
,sum(SDP.QUANTITY * SDP.VALUE_PRICE) as TOTAL_PRICE
,sum(SDP.QUANTITY * SDP.DISCOUNT_PRICE) as DISCOUNT_PRICE
,sum(SDP.QUANTITY * SDP.CUSTOMER_PRICE) as CUSTOMER_PRICE
,sum(SDP.QUANTITY * SDP.VALUE_VAT) as VAT
,PCT.NAME as PRICE_CATALOG_NAME
from SALES S
left join
(
select
    SDP.DOCUMENT_NO,
    SDP.QUANTITY,
    SDP.ITEM_CODE,
    SDP.CATALOG_CODE,
    SDP.CUSTOMER_PRICE,
    SDP.VALUE_VAT,
    SDP.DISCOUNT_PRICE,
    SDP.BASE_PRICE,
    SDP.VALUE_PRICE
  from
  SALES_DTL_PRODUCT SDP
  union
  select
    SDBD.DOCUMENT_NO,
    SDBD.QUANTITY,
    SDBD.ITEM_CODE,
    SDBD.CATALOG_CODE,
    (SDBD.VALUE_VAT + SDBD.VALUE_PRICE) as CUSTOMER_PRICE,
    SDBD.VALUE_VAT,
    SDBD.DISCOUNT_PRICE,
    SDBD.BASE_PRICE,
    SDBD.VALUE_PRICE
  from SALES_DTL_BUNDLE_DTL SDBD
  left join SALES_DTL_BUNDLE SDB on SDB.DOCUMENT_NO=SDBD.DOCUMENT_NO
  union
  select
    SDS.DOCUMENT_NO,
    SDS.QUANTITY,
    SDS.SERVICE_CODE as ITEM_CODE,
    SDS.CATALOG_CODE,
    SDS.CUSTOMER_PRICE,
    SDS.VALUE_VAT,
    SDS.DISCOUNT_PRICE,
    SDS.BASE_PRICE,
    SDS.VALUE_PRICE
  from SALES_DTL_SERVICE SDS
)
SDP on S.DOCUMENT_NO=SDP.DOCUMENT_NO
left JOIN BG on BG.BG_CODE=S.BG_CODE
INNER join PRODUCT P on P.ITEM_CODE=SDP.ITEM_CODE
INNER JOIN PRICE_CATALOG PCT on PCT.CATALOG_CODE=SDP.CATALOG_CODE
where S.DOCUMENT_DATE >= to_date($P{P_START_DATE}||',00:00','dd-mm-yyyy,HH24:MI')
and S.DOCUMENT_DATE <= to_date($P{P_END_DATE}||',23:59','dd-mm-yyyy,HH24:MI')
and BG.BG_CODE like $P{P_DEALER_CODE}
and BG.BRANCH_CODE like $P{P_LOCATION}
and BG.BG_TYPE_CODE like $P{P_BG_TYPE}
and S.STATUS_CODE = 'PAID'
group by S.DOCUMENT_DATE,S.DOCUMENT_NO,SDP.ITEM_CODE,PCT.NAME
)
union
(
select
S.DOCUMENT_DATE as SALES_DATE
,S.DOCUMENT_NO as INVOICE_NO
,SDP.ITEM_CODE
,sum(SDP.QUANTITY) as QUANTITY
,sum(SDP.QUANTITY * SDP.VALUE_PRICE) as TOTAL_PRICE
,sum(SDP.QUANTITY * SDP.DISCOUNT_PRICE) as DISCOUNT_PRICE
,sum(SDP.QUANTITY * SDP.CUSTOMER_PRICE) as CUSTOMER_PRICE
,sum(SDP.QUANTITY * SDP.VALUE_VAT) as VAT
,PCT.NAME as PRICE_CATALOG_NAME
from AGENT_SALES S
left join
(
  select
    ASD.DOCUMENT_NO,
    ASD.QUANTITY,
    ASD.ITEM_CODE,
    ASD.CATALOG_CODE,
    ASD.CUSTOMER_PRICE,
    ASD.VALUE_VAT,
    ASD.DISCOUNT_PRICE,
    ASD.BASE_PRICE,
    ASD.VALUE_PRICE,
    ASD.AGENT_ID
  from AGENT_SALES_DTL ASD
)
SDP on S.DOCUMENT_NO=SDP.DOCUMENT_NO
left JOIN BG on BG.BG_CODE=S.BG_CODE
INNER join PRODUCT P on P.ITEM_CODE=SDP.ITEM_CODE
INNER JOIN PRICE_CATALOG PCT on PCT.CATALOG_CODE=SDP.CATALOG_CODE
where S.DOCUMENT_DATE >= to_date($P{P_START_DATE}||',00:00','dd-mm-yyyy,HH24:MI')
and S.DOCUMENT_DATE <= to_date($P{P_END_DATE}||',23:59','dd-mm-yyyy,HH24:MI')
and BG.BG_CODE like $P{P_DEALER_CODE}
and BG.BRANCH_CODE like $P{P_LOCATION}
and BG.BG_TYPE_CODE like $P{P_BG_TYPE}
group by S.DOCUMENT_DATE,S.DOCUMENT_NO,SDP.ITEM_CODE,PCT.NAME
)
order by SALES_DATE,INVOICE_NO]]>
