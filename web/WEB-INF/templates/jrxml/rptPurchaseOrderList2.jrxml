	<queryString>
		<![CDATA[SELECT
    dbo.corp.corp_name,
    dbo.corp.corp_id,
    dbo.po.ponumber,
    dbo.po_detail.productcode,
    dbo.po_detail.qty,
    dbo.po_detail.producttype,
    dbo.product.product_name
FROM
    dbo.po
INNER JOIN dbo.po_detail
ON
    (
        dbo.po.ponumber = dbo.po_detail.ponumber
    )
INNER JOIN dbo.corp
ON
    (
        dbo.po.corpid = dbo.corp.corp_id
    )
INNER JOIN dbo.product
ON
    (
        dbo.po_detail.productcode = dbo.product.product_code
    )
WHERE
     po.wh_code like $P{P_DOC_NO}]]>
	</queryString>
