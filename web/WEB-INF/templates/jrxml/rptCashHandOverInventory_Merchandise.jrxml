	<field name="NAME" class="java.lang.String"/>
	<field name="WH_CODE" class="java.lang.String"/>
	<field name="BALANCE" class="java.math.BigDecimal"/>
	<variable name="ROW_NUMBER" class="java.lang.Integer">
