	<queryString>
		<![CDATA[SELECT
    dbo.putaway_detail.product_code,
    dbo.putaway_detail.wh_code,
    dbo.putaway_detail.location_code,
    dbo.wh_location.location_name,
    dbo.putaway_detail.expired_date,
    dbo.product.product_name,
    dbo.wh_location.corp_id,
    dbo.corp.corp_name
FROM
    dbo.putaway_detail
INNER JOIN dbo.wh_location
ON
    (
        dbo.putaway_detail.location_code = dbo.wh_location.location_id
    )
INNER JOIN dbo.product
ON
    (
        dbo.putaway_detail.product_id = dbo.product.product_id
    )
INNER JOIN dbo.corp
ON
    (
        dbo.wh_location.corp_id = dbo.corp.corp_id
    )
WHERE
     wh_location.wh_code = $P{P_DOC_NO}]]>
	</queryString>
