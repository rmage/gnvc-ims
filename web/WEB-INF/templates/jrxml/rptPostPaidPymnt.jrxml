	<queryString>
		<![CDATA[SELECT to_char(P.DOCUMENT_DATE,'DD-MM-YY') AS PAYMENT_DATE, U.NAME AS AGENT_NAME, P.BG_CODE AS BG_CODE, P.INVOICE_NO AS INVOICE, P.DOCUMENT_NO AS RECEIPT
, P.MSISDN AS MSISDN,CASE WHEN P.PAYMENT_TYPE IN ('FULL_PAID','LESS_PAID') THEN 'INVOICING' ELSE P.PAYMENT_TYPE END AS PAYMENT_CATEGORY
,'' AS PAYMENT_LEVEL,D.AMOUNT AS PAYMENT_AMOUNT
,CASE WHEN D.METHOD_CODE IN ('DEPOSIT') THEN D.AMOUNT ELSE 0 END AS DEPOSIT_FOR_PAYMENT
,CASE WHEN D.METHOD_CODE IN ('CASH') THEN D.AMOUNT ELSE 0 END AS CASH
,CASE WHEN D.METHOD_CODE IN ('CREDIT_CARD') THEN D.AMOUNT ELSE 0 END AS CREDIT_CARD
,CASE WHEN D.METHOD_CODE IN ('DEBIT_CARD') THEN D.AMOUNT ELSE 0 END AS DEBIT_CARD
FROM POSTPAID P
INNER JOIN POSTPAID_DTL D ON P.DOCUMENT_NO=D.POSTPAID_DOCUMENT_NO
INNER JOIN "USER" U ON U.USER_ID=D.CREATED_BY
WHERE
P.PAYMENT_TYPE NOT LIKE '%CANCEL%'
AND D.TRANS_DATE >= to_date($P{P_START_DATE},'dd-MM-yy')
and D.TRANS_DATE <= to_date($P{P_END_DATE},'dd-MM-yy')

and P.BG_CODE like $P{P_DEALER_CODE}
and D.METHOD_CODE like '%%' -- parameter P_PAYMENT_METHOD
and CASE WHEN P.PAYMENT_TYPE IN ('FULL_PAID','LESS_PAID') THEN 'INVOICING' ELSE P.PAYMENT_TYPE END like '%%'
 -- parameter P_PAYMENT_CATEGORY
ORDER BY P.BG_CODE,TRANS_TYPE_CODE ,D.METHOD_CODE, D.TRANS_DATE, U.NAME]]>
	</queryString>
