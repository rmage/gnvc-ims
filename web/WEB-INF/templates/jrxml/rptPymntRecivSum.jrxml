	<queryString>
		<![CDATA[SELECT trunc(PAYMENT_DATE) as PAYMENT_DATE,BG_CODE,
       DEPOSIT_CASH,DEPOSIT_CREDIT_CARD,DEPOSIT_DEBIT_CARD,
       POSTPAID_CASH,POSTPAID_CREDIT_CARD,POSTPAID_DEBIT_CARD,POSTPAID_DEPOSIT,
       SALES_CASH,SALES_CREDIT_CARD,SALES_DEBIT_CARD,
       POSTPAID_CASH+SALES_CASH+DEPOSIT_CASH AS TOTAL_CASH,
       POSTPAID_CREDIT_CARD+SALES_CREDIT_CARD+DEPOSIT_CREDIT_CARD AS TOTAL_CREDIT_CARD,
       POSTPAID_DEBIT_CARD+SALES_DEBIT_CARD+DEPOSIT_DEBIT_CARD AS TOTAL_DEBIT_CARD,
       POSTPAID_DEPOSIT AS TOTAL_DEPOSIT
FROM (
select
	BG_CODE,
	trunc(PAYMENT_DATE) as PAYMENT_DATE,
  MAX( DECODE(RETAIL_OR_POSTPAID,'DEPOSIT',DEPOSIT_CASH,0) ) DEPOSIT_CASH,
  MAX( DECODE(RETAIL_OR_POSTPAID,'DEPOSIT',DEPOSIT_CREDIT_CARD,0) ) DEPOSIT_CREDIT_CARD,
  MAX( DECODE(RETAIL_OR_POSTPAID,'DEPOSIT',DEPOSIT_DEBIT_CARD,0) ) DEPOSIT_DEBIT_CARD,
  MAX( DECODE(RETAIL_OR_POSTPAID,'POSTPAID',CASH,0) ) POSTPAID_CASH,
  MAX( DECODE(RETAIL_OR_POSTPAID,'POSTPAID',CREDIT_CARD,0) ) POSTPAID_CREDIT_CARD,
  MAX( DECODE(RETAIL_OR_POSTPAID,'POSTPAID',DEBIT_CARD,0) ) POSTPAID_DEBIT_CARD,
  MAX( DECODE(RETAIL_OR_POSTPAID,'POSTPAID',DEPOSIT,0) ) POSTPAID_DEPOSIT,
  MAX( DECODE(RETAIL_OR_POSTPAID,'SALES',CASH,0) ) SALES_CASH,
  MAX( DECODE(RETAIL_OR_POSTPAID,'SALES',CREDIT_CARD,0) ) SALES_CREDIT_CARD,
  MAX( DECODE(RETAIL_OR_POSTPAID,'SALES',DEBIT_CARD,0) ) SALES_DEBIT_CARD
  FROM
(
select
	BG_CODE,
	trunc(PAYMENT_DATE) as PAYMENT_DATE,
  CASE WHEN METHOD_CODE IN ('CASH') AND TRANS_TYPE_CODE NOT IN ('DEPOSIT') THEN PAYMENT_AMOUNT ELSE 0 END AS CASH,
  CASE WHEN METHOD_CODE IN ('CREDIT_CARD') AND TRANS_TYPE_CODE NOT IN ('DEPOSIT') THEN PAYMENT_AMOUNT ELSE 0 END AS CREDIT_CARD,
  CASE WHEN METHOD_CODE IN ('DEBIT_CARD') AND TRANS_TYPE_CODE NOT IN ('DEPOSIT') THEN PAYMENT_AMOUNT ELSE 0 END AS DEBIT_CARD,
  CASE WHEN METHOD_CODE IN ('DEPOSIT') THEN PAYMENT_AMOUNT ELSE 0 END AS DEPOSIT,
  CASE WHEN METHOD_CODE IN ('CASH') AND TRANS_TYPE_CODE IN ('DEPOSIT') AND RETAIL_OR_POSTPAID IN ('DEPOSIT') THEN PAYMENT_AMOUNT ELSE 0 END AS DEPOSIT_CASH,
  CASE WHEN METHOD_CODE IN ('CREDIT_CARD') AND TRANS_TYPE_CODE IN ('DEPOSIT')  AND RETAIL_OR_POSTPAID IN ('DEPOSIT') THEN PAYMENT_AMOUNT ELSE 0 END AS DEPOSIT_CREDIT_CARD,
  CASE WHEN METHOD_CODE IN ('DEBIT_CARD') AND TRANS_TYPE_CODE IN ('DEPOSIT')  AND RETAIL_OR_POSTPAID IN ('DEPOSIT') THEN PAYMENT_AMOUNT ELSE 0 END AS DEPOSIT_DEBIT_CARD,
  RETAIL_OR_POSTPAID
  from (

  select
	BG_CODE,
	trunc(PAYMENT_DATE) as PAYMENT_DATE,
	sum(PAYMENT_AMOUNT) as PAYMENT_AMOUNT,
	METHOD_CODE,RETAIL_OR_POSTPAID,TRANS_TYPE_CODE
from (
SELECT
S.BG_CODE AS BG_CODE
,trunc(SP.DOCUMENT_DATE) AS PAYMENT_DATE
,SPD.AMOUNT AS PAYMENT_AMOUNT
,M.METHOD_CODE AS METHOD_CODE
,'SALES' AS RETAIL_OR_POSTPAID
,'SALES'  AS TRANS_TYPE_CODE
FROM SALES S
INNER JOIN SALES_PAYMENT SP ON S.DOCUMENT_NO=SP.SALES_DOCUMENT_NO
INNER JOIN SALES_PAYMENT_DTL SPD ON SP.DOCUMENT_NO=SPD.DOCUMENT_NO
INNER JOIN PAYMENT_METHOD M ON SPD.METHOD_CODE=M.METHOD_CODE
WHERE S.STATUS_CODE = 'PAID'
UNION ALL
SELECT
S.BG_CODE AS BG_CODE
,trunc(S.DOCUMENT_DATE) AS PAYMENT_DATE
,SPD.AMOUNT AS PAYMENT_AMOUNT
,M.METHOD_CODE AS METHOD_CODE
,'POSTPAID' AS RETAIL_OR_POSTPAID
,CASE WHEN S.PAYMENT_TYPE IN ('FULL_PAID','LESS_PAID') THEN 'INVOICING' ELSE S.PAYMENT_TYPE END AS TRANS_TYPE_CODE
FROM POSTPAID S
INNER JOIN POSTPAID_DTL SPD ON S.DOCUMENT_NO=SPD.POSTPAID_DOCUMENT_NO
INNER JOIN PAYMENT_METHOD M ON SPD.METHOD_CODE=M.METHOD_CODE
union ALL
SELECT
S.BG_CODE AS BG_CODE
,trunc(S.DOCUMENT_DATE) AS PAYMENT_DATE
,SPD.AMOUNT AS PAYMENT_AMOUNT
,M.METHOD_CODE AS METHOD_CODE
,'DEPOSIT' AS RETAIL_OR_POSTPAID
,CASE WHEN S.PAYMENT_TYPE  IN ('DEPOSIT') THEN 'DEPOSIT'
END AS TRANS_TYPE_CODE
FROM POSTPAID S
INNER JOIN POSTPAID_DTL SPD ON S.DOCUMENT_NO=SPD.POSTPAID_DOCUMENT_NO
INNER JOIN PAYMENT_METHOD M ON SPD.METHOD_CODE=M.METHOD_CODE
WHERE s.payment_type IN ('DEPOSIT')
)
where to_date(PAYMENT_DATE,'dd-MM-YY') >= to_date($P{P_START_DATE},'dd-mm-YY')
and to_date(PAYMENT_DATE,'dd-MM-YY') <= to_date($P{P_END_DATE},'dd-mm-YY')
and BG_CODE like $P{P_DEALER_CODE}
group by BG_CODE, trunc(PAYMENT_DATE), METHOD_CODE, RETAIL_OR_POSTPAID, TRANS_TYPE_CODE
)
)
group by BG_CODE, trunc(PAYMENT_DATE)
order by BG_CODE, trunc(PAYMENT_DATE)
)]]>
