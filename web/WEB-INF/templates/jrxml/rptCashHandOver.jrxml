	<queryString>
		<![CDATA[select cash_move.document_no, bg.Name,bg.bg_code, bg.address1, bg.address2,
--cash.cash_code,
cash_move.trans_type_code, cash_move.user_from, ussr.user_id as user_id_from, ussr.name as user_name_from,

cash_move.user_to, usr.user_id as user_id_to, usr.name as user_name_to, cash_move.confirm_date, cash_move.confirm_status,
cash_move_dtl.cash_type_code, cash_move_dtl.method_code, cash_move_dtl.amount
from bg
inner join cash on bg.bg_code= cash.bg_code
inner join cash_move on cash.cash_code = cash_move.cash_code
inner join cash_move_dtl on cash_move.document_no = cash_move_dtl.document_no
inner join "USER" usr on cash_move.user_to = usr.user_id
inner join "USER" ussr on cash_move.user_from = ussr.user_id

where cash_move.confirm_status like '%Y%'
and cash_move.trans_type_code like '%CASH_HANDOVER%'
and bg.bg_code like $P{P_DEALER_CODE}
and to_date(cash_move.confirm_date,'dd-mm-YYYY') like to_date($P{P_CONFIRM_DATE},'dd-mm-YYYY')]]>
	</queryString>
