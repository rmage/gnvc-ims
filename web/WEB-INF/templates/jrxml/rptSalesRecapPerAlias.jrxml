	<parameter name="P_CATALOG_CODE" class="java.lang.String"/>
	<queryString>
		<![CDATA[select
  '--' as DELIM,
  ALIAS_NAME,
  CATEGORY_NAME,
  CATALOG_CODE,
  CATALOG_NAME,
  PRICE_CATALOG_NAME,
  UNIT_PRICE,
  sum(QUANTITY) as QUANTITY,
  sum(VALUE_PRICE) as SUM_VALUE_PRICE,
  sum(VAT) as SUM_VAT,
  sum(DISCOUNT_PRICE) as SUM_DISCOUNT_PRICE,
  sum(CUSTOMER_PRICE) as SUM_CUSTOMER_PRICE
from (
select
trunc(S.DOCUMENT_DATE) as SALES_DATE,
--,S.DOCUMENT_NO as INVOICE_NO
BG.BG_CODE,
BG.BRANCH_CODE,
BG.BG_TYPE_CODE,
PA.NAME as ALIAS_NAME
,PC.NAME as CATEGORY_NAME
,SDP.ITEM_CODE
,P.CATALOG_CODE
,PDC.NAME as CATALOG_NAME
,PAC.NAME as PRICE_CATALOG_NAME
,sum(SDP.QUANTITY) as QUANTITY
,SDP.CUSTOMER_PRICE as UNIT_PRICE
,sum(SDP.QUANTITY * SDP.VALUE_PRICE) as VALUE_PRICE
,sum(SDP.QUANTITY * SDP.VALUE_VAT) as VAT
,sum(SDP.QUANTITY * SDP.DISCOUNT_PRICE) as DISCOUNT_PRICE
,sum(SDP.QUANTITY * SDP.CUSTOMER_PRICE) as CUSTOMER_PRICE
from SALES S
left join
(
select
    SDP.DOCUMENT_NO,
    SDP.QUANTITY,
    SDP.ITEM_CODE,
    SDP.CATALOG_CODE,
    SDP.CUSTOMER_PRICE,
    SDP.VALUE_VAT,
    SDP.DISCOUNT_PRICE,
    SDP.BASE_PRICE,
    SDP.VALUE_PRICE
  from
  SALES_DTL_PRODUCT SDP
  union
  select
    SDBD.DOCUMENT_NO,
    SDBD.QUANTITY,
    SDBD.ITEM_CODE,
    SDBD.CATALOG_CODE,
    (SDBD.VALUE_VAT + SDBD.VALUE_PRICE) as CUSTOMER_PRICE,
    SDBD.VALUE_VAT,
    SDBD.DISCOUNT_PRICE,
    SDBD.BASE_PRICE,
    SDBD.VALUE_PRICE
  from SALES_DTL_BUNDLE_DTL SDBD
  left join SALES_DTL_BUNDLE SDB on SDB.DOCUMENT_NO=SDBD.DOCUMENT_NO
  union
  select
    SDS.DOCUMENT_NO,
    SDS.QUANTITY,
    SDS.SERVICE_CODE as ITEM_CODE,
    SDS.CATALOG_CODE,
    SDS.CUSTOMER_PRICE,
    SDS.VALUE_VAT,
    SDS.DISCOUNT_PRICE,
    SDS.BASE_PRICE,
    SDS.VALUE_PRICE
  from SALES_DTL_SERVICE SDS
)
SDP on S.DOCUMENT_NO=SDP.DOCUMENT_NO
INNER JOIN BG on BG.BG_CODE=S.BG_CODE
INNER JOIN PRODUCT P on P.ITEM_CODE=SDP.ITEM_CODE
INNER JOIN PRODUCT_ALIAS PA on PA.CODE=P.ALIAS
LEFT JOIN PRODUCT_CATEGORY PC on PC.CATEGORY_CODE=P.CATEGORY_CODE
INNER JOIN PRICE_CATALOG PAC on PAC.CATALOG_CODE=SDP.CATALOG_CODE
INNER JOIN PRODUCT_CATALOG PDC on PDC.CATALOG_CODE=P.CATALOG_CODE
group by trunc(S.DOCUMENT_DATE), BG.BG_CODE, BG.BRANCH_CODE, BG.BG_TYPE_CODE, PA.NAME, PC.NAME, SDP.ITEM_CODE, P.CATALOG_CODE, PDC.NAME, PAC.NAME, SDP.CUSTOMER_PRICE
union
select
trunc(S.DOCUMENT_DATE) as SALES_DATE,
--,S.DOCUMENT_NO as INVOICE_NO
BG.BG_CODE,
BG.BRANCH_CODE,
BG.BG_TYPE_CODE,
PA.NAME as ALIAS_NAME
,PC.NAME as CATEGORY_NAME
,SDP.ITEM_CODE
,P.CATALOG_CODE
,PDC.NAME as CATALOG_NAME
,PAC.NAME as PRICE_CATALOG_NAME
,sum(SDP.QUANTITY) as QUANTITY
,SDP.CUSTOMER_PRICE as UNIT_PRICE
,sum(SDP.QUANTITY * SDP.VALUE_PRICE) as VALUE_PRICE
,sum(SDP.QUANTITY * SDP.VALUE_VAT) as VAT
,sum(SDP.QUANTITY * SDP.DISCOUNT_PRICE) as DISCOUNT_PRICE
,sum(SDP.QUANTITY * SDP.CUSTOMER_PRICE) as CUSTOMER_PRICE
from AGENT_SALES S
left join
(
  select
    ASD.DOCUMENT_NO,
    ASD.QUANTITY,
    ASD.ITEM_CODE,
    ASD.CATALOG_CODE,
    ASD.CUSTOMER_PRICE,
    ASD.VALUE_VAT,
    ASD.DISCOUNT_PRICE,
    ASD.BASE_PRICE,
    ASD.VALUE_PRICE
  from AGENT_SALES_DTL ASD
)
SDP on S.DOCUMENT_NO=SDP.DOCUMENT_NO
INNER JOIN BG on BG.BG_CODE=S.BG_CODE
INNER JOIN PRODUCT P on P.ITEM_CODE=SDP.ITEM_CODE
INNER JOIN PRODUCT_ALIAS PA on PA.CODE=P.ALIAS
LEFT JOIN PRODUCT_CATEGORY PC on PC.CATEGORY_CODE=P.CATEGORY_CODE
INNER JOIN PRICE_CATALOG PAC on PAC.CATALOG_CODE=SDP.CATALOG_CODE
INNER JOIN PRODUCT_CATALOG PDC on PDC.CATALOG_CODE=P.CATALOG_CODE
group by trunc(S.DOCUMENT_DATE),BG.BG_CODE,BG.BRANCH_CODE,BG.BG_TYPE_CODE,PA.NAME,PC.NAME,SDP.ITEM_CODE,P.CATALOG_CODE,PDC.NAME
,PAC.NAME,SDP.CUSTOMER_PRICE
)
where SALES_DATE >= to_date($P{P_START_DATE}||',00:00:00','dd-mm-yyyy,HH24:MI:SS')
and SALES_DATE <= to_date($P{P_END_DATE}||',23:59:59','dd-mm-yyyy,HH24:MI:SS')
and BG_CODE like $P{P_DEALER_CODE}
and BRANCH_CODE like $P{P_LOCATION}
and BG_TYPE_CODE like $P{P_BG_TYPE}
and CATALOG_CODE like $P{P_CATALOG_CODE}
group by ALIAS_NAME,CATEGORY_NAME,UNIT_PRICE,PRICE_CATALOG_NAME,CATALOG_CODE,CATALOG_NAME
order by PRICE_CATALOG_NAME asc,ALIAS_NAME,CATEGORY_NAME]]>
