	<queryString>
		<![CDATA[SELECT
BG.BG_CODE AS BG_CODE
,BG.NAME as BG_NAME
,P.ITEM_CODE AS ITEM_CODE
,S.START_DATE as DOCUMENT_DATE
,BG."NAME" AS BG_NAME
,B.BRANCH_CODE AS AREA_CODE
,B."NAME" AS AREA_NAME
,WH.WH_CODE AS WH_CODE
,WH."NAME" AS WH_NAME
,P."NAME" AS ITEM_NAME
,SWH.BALANCE AS QTY
,PC."NAME" AS ITEM_TYPE
,S.START_DATE as CUR_DATE
FROM STOCK S
INNER JOIN BG BG ON S.BG_CODE=BG.BG_CODE
INNER JOIN BRANCH B ON BG.BRANCH_CODE=B.BRANCH_CODE
INNER JOIN STOCK_WH SWH ON S.STOCK_CODE=SWH.STOCK_CODE
INNER JOIN WH WH ON SWH.WH_CODE = WH.WH_CODE
INNER JOIN PRODUCT P ON SWH.ITEM_CODE = P.ITEM_CODE
INNER JOIN PRODUCT_CATALOG PC ON P.CATALOG_CODE=PC.CATALOG_CODE
where S.IS_CURRENT='Y'
and WH.WH_CODE in ('WHT', 'WH0', 'WH1')
and BG.BRANCH_CODE like $P{P_LOCATION}
and BG.BG_CODE like $P{P_DEALER_CODE}
and BG.BG_TYPE_CODE like $P{P_BG_TYPE}
and PC.CATALOG_CODE like $P{P_ITEM_TYPE}
order by BG.BG_CODE, P.ITEM_CODE]]>
	</queryString>
