	<parameter name="P_DOC_NO" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
      B.NAME as BUNDLE_NAME,
      SDB.QUANTITY as BUNDLE_QUANTITY,
      SDB.CUSTOMER_PRICE as BUNDLE_PRICE,
      S.DOCUMENT_NO,
      (nvl(P.NAME,SVC.NAME)) as NAMA_BARANG,
      SD.SN_START,
      SD.SN_END,
      SD.QUANTITY as KUANTITAS,
      SD.DISCOUNT_PRICE as DISKON,
      SD.VALUE_VAT as PPN,
      SD.CUSTOMER_PRICE,
      SD.VALUE_PRICE,
      S.VALUE_PRICE as TOTAL_VALUE,
      S.DISCOUNT_PRICE as TOTAL_DISCOUNT,
      S.CUSTOMER_PRICE as TOTAL_TAGIHAN,
      S.VALUE_VAT as TOTAL_PPN,
      BG.NAME as BG_NAME,
      BG.ADDRESS1 as BG_ADDR,
      A.NAME as AGENT_NAME,
      C.NAME as CUST_NAME,
      C.NPWP as CUST_NPWP,
      C.ADDRESS as CUST_ADDR,
      C.PHONE as CUST_PHONE,
      SD.IDENT_NO
FROM SALES S
left join
(
  select
    null as BUNDLE_CODE,
    SDP.DOCUMENT_NO,
    SDP.QUANTITY,
    SDP.ITEM_CODE,
    SDP.CUSTOMER_PRICE,
    SDP.VALUE_PRICE,
    SDP.DISCOUNT_PRICE,
    SDP.VALUE_VAT,
    SDI.IDENT_NO,
    SDR.SN_START,
    SDR.SN_END
  from
  SALES_DTL_PRODUCT SDP
  left join SALES_DTL_IDENT SDI on SDI.ITEM_CODE=SDP.ITEM_CODE and SDI.DOCUMENT_NO=SDP.DOCUMENT_NO
  left join SALES_DTL_RANGE SDR on SDR.ITEM_CODE=SDP.ITEM_CODE and SDR.DOCUMENT_NO=SDP.DOCUMENT_NO
  union
  select
    SDB.BUNDLE_CODE,
    SDBD.DOCUMENT_NO,
    SDBD.QUANTITY,
    SDBD.ITEM_CODE,
    (SDBD.VALUE_PRICE+SDBD.VALUE_VAT) as CUSTOMER_PRICE,
    SDBD.VALUE_PRICE,
    SDBD.DISCOUNT_PRICE,
    SDBD.VALUE_VAT,
    SDBDI.IDENT_NO,
    SDBDR.SN_START,
    SDBDR.SN_END
  from SALES_DTL_BUNDLE SDB
  inner join SALES_DTL_BUNDLE_DTL SDBD on SDB.DOCUMENT_NO=SDBD.DOCUMENT_NO and SDB.BUNDLE_CODE=SDBD.BUNDLE_CODE
  left join SALES_DTL_BUNDLE_DTL_IDENT SDBDI on SDBDI.DOCUMENT_NO=SDBD.DOCUMENT_NO and SDBDI.BUNDLE_CODE=SDBD.BUNDLE_CODE
  and SDBDI.ITEM_CODE=SDBD.ITEM_CODE
  left join SALES_DTL_BUNDLE_RANGE SDBDR on SDBDR.DOCUMENT_NO=SDBD.DOCUMENT_NO
  and SDBDR.ITEM_CODE=SDBD.ITEM_CODE
  INNER JOIN BUNDLE B on B.BUNDLE_CODE=SDB.BUNDLE_CODE
  union
  select
    null as BUNDLE_CODE,
    SDS.DOCUMENT_NO,
    SDS.QUANTITY,
    SDS.SERVICE_CODE as ITEM_CODE,
    SDS.CUSTOMER_PRICE,
    SDS.VALUE_PRICE,
    SDS.DISCOUNT_PRICE,
    SDS.VALUE_VAT,
    null as IDENT_NO,
    null as SN_START,
    null as SN_END
  from SALES_DTL_SERVICE SDS
) SD on SD.DOCUMENT_NO=S.DOCUMENT_NO
left join PRODUCT P on SD.ITEM_CODE=P.ITEM_CODE
left join SERVICE SVC on SD.ITEM_CODE=SVC.SERVICE_CODE
left join BUNDLE B on B.BUNDLE_CODE=SD.BUNDLE_CODE
left join SALES_DTL_BUNDLE SDB on SDB.DOCUMENT_NO=SD.DOCUMENT_NO and SDB.BUNDLE_CODE=SD.BUNDLE_CODE
left join AGENT A on A.AGENT_ID=S.AGENT_ID
left join CUSTOMER C on C.CUSTOMER_ID=S.CUSTOMER_ID
left join BG on BG.BG_CODE=S.BG_CODE
where S.DOCUMENT_NO=$P{P_DOC_NO}
order by BUNDLE_NAME]]>
