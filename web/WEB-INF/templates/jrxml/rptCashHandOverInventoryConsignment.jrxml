	<queryString>
		<![CDATA[select to_char(sysdate,'DD-MON-YY') as tanggal, s.bg_code, swh.item_code, pa.name, swh.wh_code, swh.balance from stock s
left join stock_wh swh on s.stock_code=swh.stock_code
left join product p on swh.item_code=p.item_code
left join product_alias pa on p.alias=pa.code
where p.catalog_code='CS' and swh.wh_code not in ('WHS')
and s.bg_code like $P{P_DEALER_CODE}
order by s.bg_code, swh.item_code, swh.wh_code]]>
