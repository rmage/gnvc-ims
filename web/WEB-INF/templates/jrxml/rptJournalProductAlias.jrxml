	<queryString>
		<![CDATA[select  s.bg_code as bg_code,
jdp.document_no as invoice_no,
to_char(s.document_date,'DD-MM-YY') as sales_date,
jdp.item_Code as item_code,
jdp.journal_no as journal_no,
c.name as coa_name,
jdp.acc_no as acc,
jdp.catalog_code as catalog,
jdp.debet as debet,
jdp.credit as credit
from journal j
left join journal_dtl_product jdp on j.journal_no=jdp.journal_no
left join sales s on s.document_no=jdp.document_no
left join coa c on c.acc_no=jdp.acc_no
where
to_date(j.sales_from,'DD-MM-YY') >= to_date($P{P_START_DATE},'DD-MM-YY')
AND to_date(j.sales_from,'DD-MM-YY') <= to_date($P{P_END_DATE},'DD-MM-YY')
order by bg_Code, invoice_no, item_code, debet]]>
	</queryString>
