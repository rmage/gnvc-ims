	<queryString>
		<![CDATA[SELECT
    dbo.vbalanceputawaydetail.balance,
    dbo.vbalanceputawaydetail.location_code,
    dbo.wh_location.location_name,
    dbo.wh_location_detail.product_id,
 dbo.wh_location_detail.product_code,
    dbo.product.product_name,
    dbo.wh_location.wh_code,
    dbo.wh_location.corp_id,
dbo.wh_location.locating_zone,
    dbo.corp.corp_name
FROM
    dbo.wh_location
INNER JOIN dbo.wh_location_detail
ON
    (
        dbo.wh_location.location_id = dbo.wh_location_detail.location_id
    )
INNER JOIN dbo.vbalanceputawaydetail
ON
    (
        dbo.wh_location.location_code = dbo.vbalanceputawaydetail.location_code
    )
INNER JOIN dbo.product
ON
    (
        dbo.wh_location_detail.product_id = dbo.product.product_id
    )
INNER JOIN dbo.corp
ON
    (
        dbo.wh_location.corp_id = dbo.corp.corp_id
    )
INNER JOIN dbo.wh
ON
    (
        dbo.wh.wh_code = dbo.wh_location.wh_code
    )
where wh.wh_code = $P{P_DOC_NO}]]>
