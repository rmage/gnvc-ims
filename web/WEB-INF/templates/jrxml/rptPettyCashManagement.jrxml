	</parameter>
	<parameter name="P_USERNAME" class="java.lang.String"/>
	<parameter name="P_BG_TYPE" class="java.lang.String"/>
	<queryString>
		<![CDATA[Select
P.BG_CODE AS BG_CODE
,BG.NAME as BG_NAME
,trunc(P.START_DATE) as PETTY_DATE
,P.PETTY_CODE
,POT."NAME" AS EXPENSE_TYPE
,PIO.DESCRIPTION AS DESCRIPTION
,P.START_BALANCE AS OPENING_BALANCE
,P.BALANCE
,PIO.CASH_OUT AS CASH_OUT
,PIO.CASH_IN AS CASH_IN
from PETTY P
inner join(
SELECT PIN.PETTY_CODE,PIN.DESCRIPTION,PIN.AMOUNT AS CASH_IN,0 as CASH_OUT,'--' as OUT_TYPE_CODE from PETTY_IN PIN
INNER join PETTY on PETTY.PETTY_CODE = PIN.PETTY_CODE
INNER join BG on BG.BG_CODE=PETTY.BG_CODE
union
SELECT POUT.PETTY_CODE,POUT.DESCRIPTION,0 as CASH_IN,POUT.AMOUNT AS CASH_OUT,POUT.OUT_TYPE_CODE from PETTY_OUT POUT
INNER join PETTY on PETTY.PETTY_CODE = POUT.PETTY_CODE
INNER join BG on BG.BG_CODE=PETTY.BG_CODE
) PIO on PIO.PETTY_CODE=P.PETTY_CODE
LEFT JOIN
(SELECT OUT_TYPE_CODE,"NAME" from PETTY_OUT_TYPE) POT on POT.OUT_TYPE_CODE=PIO.OUT_TYPE_CODE
INNER JOIN BG on BG.BG_CODE=P.BG_CODE
where P.IS_CURRENT='Y'
and BG.BG_CODE like $P{P_DEALER_CODE}
and BG.BRANCH_CODE like $P{P_LOCATION}
and BG.BG_TYPE_CODE like $P{P_BG_TYPE}
order by BG_CODE asc,CASH_IN desc]]>
